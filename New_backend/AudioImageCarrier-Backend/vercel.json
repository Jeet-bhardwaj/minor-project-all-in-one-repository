{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "API_KEY": "@api_key",
    "UPLOAD_DIR": "/tmp/uploads",
    "TEMP_DIR": "/tmp/temp",
    "MAX_UPLOAD_SIZE_MB": "100",
    "DEFAULT_MAX_CHUNK_BYTES": "10485760",
    "CORS_ORIGINS": "[\"*\"]"
  }
}
